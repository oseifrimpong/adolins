<!DOCTYPE html>
<html>
  <head>
    <title>Adolins</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link rel="stylesheet" type="text/css" href="../assets/bootstrap.css" media="screen">
    <link rel="stylesheet" type="text/css" href="../assets/style.css" media="screen">
    <link rel="stylesheet" type="text/css" href="../assets/font-awesome.css" media="screen">
    <link rel="stylesheet" href="../assets/flexslider.css" type="text/css" media="screen"/>
    <link rel="stylesheet" href="../assets/responsive.css" type="text/css" media="screen"/>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Lobster' rel='stylesheet' type='text/css'>
  </head>

  <body>
  <%= render 'layouts/navbar' %>
  	<p class="notice"><%= notice %></p>
  	<p class="alert"><%= alert %></p>
    <%= yield %>
  <%= render 'layouts/footer'  %>
  </body>
<script type="text/javascript" src="../assets/jquery.min.js"></script>
<script type="text/javascript" src="../assets/bootstrap.js"></script>
<script type="text/javascript" src="../assets/script.js"></script>
<script src="../assets/modernizr.js"></script>
<script type="text/javascript" src="../assets/jquery.nouislider.min.js"></script>
</html>
